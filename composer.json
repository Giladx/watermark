{
  "name": "prestashop/watermark",
  "description": "PrestaShop module watermark",
  "homepage": "https://github.com/PrestaShop/watermark",
  "license": "AFL - Academic Free License (AFL 3.0)",
  "authors": [
    {
      "name": "PrestaShop SA",
      "email": "contact@prestashop.com"
    }
  ],
  "require": {
    "php": ">=5.6",
    "prestashop/circuit-breaker": "^3.0.0"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "git@github.com:jolelievre/circuit-breaker.git"
    }
  ],
  "autoload": {
    "psr-4": {
      "PrestaShop\\Module\\Watermark\\": "src/"
    },
    "classmap": ["watermark.php"],
    "exclude-from-classmap": []
  },
  "config": {
    "preferred-install": "dist",
    "prepend-autoloader": false,
    "platform": {
      "php": "5.6"
    }
  },
  "type": "prestashop-module"
}